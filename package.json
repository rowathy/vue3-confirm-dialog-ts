{
  "name": "vue3-confirm-dialog",
  "version": "0.1.3",
  "type": "module",
  "main": "./dist/vue3-confirm-dialog.js",
  "module": "./dist/vue3-snotify.es.js",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/vue3-confirm-dialog.js",
      "require": "./dist/vue3-confirm-dialog.umd.cjs"
    },
    "./style": "./dist/style.css"
  },
  "scripts": {
